{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "flutter analyze": {
      "name": "flutter analyze",
      "command": "clear && cd $ROOT_PROJECT/lib && flutter analyze",
      "runAtStart": false
    },
    "show pathes": {
      "name": "show pathes",
      "command": "clear && echo $PATH && echo && echo && echo $ROOT_FLUTTER && echo && echo $ROOT_DART_SDK && echo && echo $ROOT_CHROMIUM && echo && echo $ROOT_PROJECT && acho && echo $CHROME_EXECUTABLE && echo",
      "runAtStart": false
    },
    "whereis flutter chromium to pathList.txt": {
      "name": "whereis flutter chromium to pathList.txt",
      "command": "whereis flutter chromium > pathList.txt",
      "runAtStart": true
    },
    "flutter config": {
      "name": "flutter config",
      "command": "flutter config --clear-features && flutter config --enable-web && flutter config --no-enable-android && flutter config --no-enable-linux-desktop && flutter config --no-analytics && flutter config",
      "runAtStart": false
    },
    "flutter doctor": {
      "name": "flutter doctor",
      "command": "flutter doctor",
      "runAtStart": false
    },
    "nix-collect-garbage": {
      "name": "nix-collect-garbage",
      "command": "nix-collect-garbage",
      "runAtStart": false
    },
    "echo PATH to PATH.txt": {
      "name": "echo PATH to PATH.txt",
      "command": "echo $PATH > PATH.txt",
      "runAtStart": false
    },
    "flutter create flutter_project": {
      "name": "flutter create flutter_project",
      "command": "flutter create flutter_project --verbose --no-overwrite && rm -rv $ROOT_PROJECT/.idea  && rm -rv $ROOT_PROJECT/ios  && rm -rv $ROOT_PROJECT/macos  && rm -rv $ROOT_PROJECT/windows",
      "runAtStart": false
    }
  }       
}
