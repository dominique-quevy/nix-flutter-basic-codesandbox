{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "show pathes": {
      "name": "show pathes",
      "command": "clear && whereis flutter python3 chromium > pathList.txt && echo $PATH > PATH.txt && clear && echo $PATH && echo && echo && echo $ROOT_FLUTTER && echo && echo $ROOT_DART_SDK && echo && echo $ROOT_CHROMIUM && echo && echo $CHROME_EXECUTABLE && echo && python3 --version && echo && echo && echo $ROOT_PROJECT",
      "runAtStart": true
    },
    "flutter config - flutter doctor": {
      "name": "flutter config - flutter doctor",
      "command": "clear && flutter config --clear-features && flutter config --enable-web && flutter config --no-enable-android && flutter config --no-enable-linux-desktop && flutter config --no-analytics && flutter config && flutter doctor",
      "runAtStart": false
    },
    "nix-collect-garbage - flutter clean": {
      "name": "nix-collect-garbage - flutter clean",
      "command": "clear && nix-collect-garbage && ls $ROOT_PROJECT && cd $ROOT_PROJECT && flutter clean",
      "runAtStart": false
    },
    "dart format - flutter analyze": {
      "name": "flutter analyze",
      "command": "clear && flutter pub get && dart --disable-analytics && cd $LIB_PROJECT && dart format $ROOT_PROJECT/lib --fix --line-length=80 --follow-links --set-exit-if-changed && flutter analyze",
      "runAtStart": false
    },
    "flutter build web": {
      "name": "flutter build web",
      "command": "clear && cd $LIB_PROJECT && flutter devices && flutter pub get && flutter build web",
      "runAtStart": false
    },
    "flutter logs": {
      "name": "flutter logs",
      "command": "clear && cd $LIB_PROJECT && flutter devices && flutter logs",
      "runAtStart": false
    },
    "flutter create flutter_project - flutter web only": {
      "name": "flutter create flutter_project - flutter web only",
      "command": "clear && flutter create flutter_project --verbose --no-overwrite && rm -rv $ROOT_PROJECT/.idea && rm -rv $ROOT_PROJECT/ios && rm -rv $ROOT_PROJECT/macos && rm -rv $ROOT_PROJECT/windows",
      "runAtStart": false
    },
    "dart devtools": {
      "name": "dart devtools",
      "command": "clear && dart devtools",
      "runAtStart": false
    }
  }
}
